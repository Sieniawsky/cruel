    "express": "~4.2.0",
    "express-session": "~1.0.0",
    "hogan-express": "~0.5.2",
    "jquery": "^2.1.4",
    "lodash": "3.8.0",
    "method-override": "~1.0.0",
    "moment": "latest",
    "mongodb": "~1.4.7",
    "mongoose": "4.0.2",
    "morgan": "~1.0.0",
    "passport": "latest",
    "passport-local": "latest",
    "serve-favicon": "~2.0.1"
